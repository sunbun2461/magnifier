{
	"name": "enhanced-image-magnifier",
	"version": "2.0.0",
	"description": "Complete vanilla JavaScript image magnifier inspired by React Image Magnifier Waft. Features side-by-side and in-place magnification, touch support, and extensive customization options.",
	"main": "enhanced-magnifier.js",
	"scripts": {
		"demo": "open demo.html",
		"example": "open magnifier-only.html",
		"gallery": "open gallery-example.html",
		"serve": "python -m http.server 8000",
		"sass": "sass sass:css --watch",
		"sass:build": "sass sass:css --style compressed",
		"start": "npm run sass & npm run serve"
	},
	"keywords": [
		"image",
		"magnifier",
		"zoom",
		"vanilla-javascript",
		"ecommerce",
		"product-zoom",
		"image-zoom",
		"magnify",
		"lens",
		"responsive",
		"touch",
		"mobile",
		"amazon",
		"flipkart",
		"gallery"
	],
